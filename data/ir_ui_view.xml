              <field name="x_studio_date_stop" invisible="1"/>
            </group>
            <group name="studio_group_e225f2_right">
              <field name="x_studio_user_id" widget="many2one_avatar_user"/>
              <field name="x_studio_currency_id" invisible="1"/>
              <field name="x_studio_value"/>
              <field name="x_studio_tag_ids" widget="many2many_tags" options="{'color_field': 'x_color'}"/>
              <field name="x_studio_company_id" groups="base.group_multi_company" options="{'no_create': True}"/>
            </group>
          </group>
          <group>
            <field name="x_studio_notes" placeholder="Type down your notes here..." nolabel="1" colspan="2"/>
          </group>
          <notebook>
            <page string="Details" name="lines">
              <field name="x_sjk_studio_line_ids_dfb79"/>
            </page>
          </notebook>
        </sheet>
        <chatter/>
      </form>
    </field>
    <field name="model">x_sjk_studio</field>
    <field name="name">Default form view for x_sjk_studio</field>
    <field name="type">form</field>
  </record>
  <record id="default_search_view__1d82cf9c-aff7-4b0e-93c8-184e7ef8c108" model="ir.ui.view" context="{'studio': True}">
    <field name="arch" type="xml">
      <search>
