  <record id="default_form_view_fo_e5e10a7b-f9d8-4799-bdf9-0beba9d07be7" model="ir.ui.view" context="{'studio': True}">
    <field name="arch" type="xml">
      <form>
        <header>
          <field name="x_studio_stage_id" widget="statusbar" options="{'clickable': '1'}"/>
        </header>
        <sheet string="SJK Studio">
          <field name="x_studio_kanban_state" widget="state_selection"/>
