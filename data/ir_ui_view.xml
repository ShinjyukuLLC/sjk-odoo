          <t t-name="card" class="flex-row">
            <aside>
              <field name="x_studio_image" widget="image" options="{&quot;zoom&quot;: true, &quot;background&quot;: true, &quot;preventClicks&quot;: false}"/>
            </aside>
            <main>
              <div name="studio_auto_kanban_header" class="d-flex">
                <field name="x_studio_priority" widget="boolean_favorite" nolabel="1"/>
                <field name="display_name" class="h5"/>
              </div>
              <div name="studio_auto_kanban_body">
                <field name="x_studio_tag_ids" options="{'color_field': 'x_color'}"/>
              </div>
              <footer>
                <field name="x_studio_value"/>
                <div class="ms-auto d-flex align-items-center">
                  <field name="x_studio_kanban_state" widget="state_selection"/>
                  <field class="ms-1" name="x_studio_user_id" widget="kanban.many2one_avatar_user"/>
                </div>
              </footer>
            </main>
          </t>
        </templates>
      </kanban>
    </field>
    <field name="model">x_tender_management</field>
    <field name="name">Default kanban view for x_tender_management</field>
    <field name="type">kanban</field>
  </record>
